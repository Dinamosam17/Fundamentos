#include <iostream>

using namespace std; 
int main()
{
    int i, N, acum = 1;

    do {
        cout << "Ingrese el numero de elementos: ";
        cin >> N;
    } while (N < 0);

    for (int i = 2; i <= N; i++)
    {
        acum = acum * i;
    }
    cout << "El factorial de " << N << " es: " << acum; 
    return 0; 
}
